PlayerSection:NewSlider("WalkSpeed", "Makes you move Faster or Slower", 500, 0, function(s) -- 1000 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

SettingsSection:NewKeybind("Toggle UI", "UI Toggle", Enum.KeyCode.F, function()
